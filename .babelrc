{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": [
            ">1%",
            "last 3 versions"
          ]
        }
      }
    ],
    "stage-2",
    "latest",
    "react", ["es2015"]
  ],
  "plugins": [
    "syntax-dynamic-import",
    "transform-class-properties", [
      "import",
      {
        "style": "css",
        "libraryName": "antd-mobile"
      }
    ]
  ]
}